
@import 'normalize'

// more info http://stylusgrid.com
@import 's-grid-settings'
@import 's-grid-functions'
// @import 's-grid-classes' <- optional, if needed

@import 'settings'
@import 'helpers'

@import 'spinner'


.container
    center(1000px)

.header
    height rem-calc(80)
    line-height rem-calc(80)

.spacing
    padding-top rem-calc(25)
    padding-bottom rem-calc(25)

.custom-grid
    grid()
    .custom-cell
        cell(1, 2)
        +below(rem-calc(768))
            stack()
